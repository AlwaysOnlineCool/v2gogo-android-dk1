<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 主界面的tabbar -->
    <style name="style_activity_tab_main_tab_bar_item">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:background">@null</item>
        <item name="android:drawablePadding">2dip</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/selector_tab_main_tab_bar_item_text_color</item>
    </style>

    <!-- 我的每一个item layout -->
    <style name="style_fragment_profile_item_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/fragment_profile_item_height</item>
        <item name="android:paddingLeft">@dimen/fragment_profile_padding_left</item>
        <item name="android:paddingRight">@dimen/fragment_profile_padding_right</item>
    </style>

    <style name="style_fragment_profile_item_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">12dip</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/app_common_black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="style_fragment_profile_setting_tip">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">28dip</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/action_bar_color</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginLeft">@dimen/fragment_profile_padding_left</item>
        <item name="android:layout_marginRight">@dimen/fragment_profile_padding_right</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="style_fragment_profile_item_arrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:contentDescription">@string/app_name</item>
        <item name="android:src">@drawable/user_icons_forward</item>
    </style>

    <style name="style_fragment_profile_item_new">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">50dip</item>
        <item name="android:contentDescription">@string/app_name</item>
        <item name="android:src">@drawable/user_icons_new</item>
    </style>

    <style name="style_fragment_home_header_item">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
